#include<stdio.h>
#include<conio.h>

void main()
	{
		int a,b,c;
		
		printf("enter the value of a \n");
		scanf("%d",&a);
		
		printf("enter the value of b \n");
		scanf("%d",&b);
		
		printf("enter the value of c \n");
		scanf("%d",&c);
		
		if(a>b&&b>c)
		{
			printf("A is greater");
		}
		else if(b>c&&c>a)
		{
			printf("B is greater");
		}
		else if(c>a&&a>b)
		{
			printf("C is greater");
		}
		
		getch();
	}	
